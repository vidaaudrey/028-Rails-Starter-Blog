<%= render 'shared/title', title: "Login" %>


<div class="row">
    <div class="col-md-6 col-md-offset-3 well">
       <%= form_tag '/login' do %>
           <p> 
                <%= label_tag :email %><br>
               <%= text_field_tag :email %>
            </p>

            <p>
                <%= label_tag :password %><br>
                <%= password_field_tag :password %>
            </p>
            <%= submit_tag "Login", class:"btn btn-success" %>
       <% end %>

    </div>
</div>